{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "recorder-worker",
	"main": "src/index.ts",
	"compatibility_date": "2025-08-06",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"ai": {
		"binding": "AI"
	},
	"browser": {
		"binding": "MYBROWSER"
	},
	"assets": {
		"directory": "./public/",
		"binding": "ASSETS"
	}
}
